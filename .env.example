# Google Gemini API Key for Mastra agents
GOOGLE_GENERATIVE_AI_API_KEY=your-gemini-api-key

# Mastra Cloud Access Token (set to 'off' to disable)
MASTRA_CLOUD_ACCESS_TOKEN=off

# Telex Webhook URL (optional - for daily scheduled messages at 8am)
# Get this from your Telex channel settings
# TELEX_WEBHOOK_URL=https://telex.im/api/webhook/your-channel-id

# Port (optional - defaults to 8080)
# PORT=8080
